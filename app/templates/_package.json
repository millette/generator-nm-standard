{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%- description %>",
  "license": "AGPL-3.0",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%- name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "standard",
    "test": "standard && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli"<% } %>
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0",
    "update-notifier": "^2.1.0"
  <% } %>},
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "coveralls": "^2.12.0",
    "nyc": "^10.1.2",
    "standard": "^9.0.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
